{
  "manifest_version": 3,
  "name": "Chrome URL Tracker",
  "version": "1.1.0",
  "description": "A Chrome extension to track URL changes.",
  "permissions": [
    "tabs",
    "storage",
    "history"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  }
}